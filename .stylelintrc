{
  "processors": [    
  ],
  "plugins": [
    "stylelint-scss"
  ],
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-sass-guidelines"
  ],
  "defaultSeverity": "warning",
  "rules": {
    "scss/dollar-variable-pattern": /./,
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "scss/at-function-pattern": .
  }
}