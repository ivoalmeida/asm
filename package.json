{
  "name": "market-monalisa",
  "version": "0.0.1",
  "description": "New verison of Market UI",
  "main": "index.tsx",
  "repository": "git@github.com:Venatus/market-monalisa.git",
  "author": "Ivo Almeida <ivo.almeida@venatusmedia.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn env:dev webpack-dev-server --mode development --open --hot",
    "build": "yarn env:prod webpack --mode production",
    "build:dev": "yarn env:dev webpack --mode development; exit 0;",
    "build:css": "node-sass src/ -o src/ ",
    "build:storybook": "build-storybook -c .storybook -o .out",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "check-types": "tsc",
    "cleanup": "rimraf node_modules; rimraf dist; rimraf './src/**/*.d.ts';",
    "format:fix": "pretty-quick --staged",
    "lint": "tslint './src/**/*.tsx'",
    "lint:css": "stylelint './src/**/*.scss'; exit 0;",
    "css:fix": "stylelint */**/*.scss --fix; exit 0;",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint lint:css",
      "pre-push": "run-s test"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".+\\.scss$": "jest-css-modules-transform",
      "^.+\\.(ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "apollo-boost": "^0.4.4",
    "apollo-link-schema": "^1.2.4",
    "classnames": "^2.2.6",
    "graphql": "^14.5.6",
    "graphql-tools": "^4.0.5",
    "react": "^16.9.0",
    "react-apollo": "^3.1.1",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addon-info": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.18",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__addon-knobs": "^5.0.4",
    "@types/storybook__react": "^4.0.2",
    "autoprefixer": "^9.6.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "bcryptjs": "^2.4.3",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "faker": "^4.1.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-css-modules-transform": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.18",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-docgen-typescript-loader": "^3.2.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-test-renderer": "^16.9.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-scss": "^3.11.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typed-css-modules-loader": "^0.0.17",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "yarn-run-all": "^3.1.1"
  }
}
